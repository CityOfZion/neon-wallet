{
  "name": "Neon",
  "version": "0.0.6",
  "main": "./main.js",
  "description": "Light wallet for NEO blockchain",
  "homepage": "https://github.com/CityOfZion/neon-wallet",
  "author": "Ethan Fast <ejhfast@gmail.com> (https://github.com/Ejhfast)",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "assets": "webpack",
    "assets-watch": "webpack --watch",
    "test": "jest",
    "test-watch": "jest --watch",
    "pack": "build --dir",
    "dist": "build"
  },
  "keywords": [],
  "license": "ISC",
  "build": {
    "appId": "com.electron.neon",
    "files": [
      "./main.js",
      "app/dist/index.html",
      "app/dist/bundle.js"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "base-x": "^3.0.2",
    "bigi": "^1.4.2",
    "bs58check": "^2.0.2",
    "buffer": "^5.0.6",
    "crypto-js": "^3.1.9-1",
    "ecurve": "^1.0.5",
    "electron-context-menu": "^0.9.1",
    "electron-json-storage": "^3.0.7",
    "electron-save-file": "^1.0.2",
    "elliptic": "^6.4.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.1-security",
    "isomorphic-fetch": "^2.2.1",
    "js-scrypt": "^0.2.0",
    "lodash": "^4.17.4",
    "neon-js": "git+https://github.com/CityOfZion/neon-js.git#8a52d07a4eda09afd1b57485deb6973409d0c342",
    "prop-types": "^15.6.0",
    "qrcode": "^0.8.2",
    "react": "^15.0.2",
    "react-ace": "^5.0.1",
    "react-data-grid": "^2.0.33",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-icons": "^2.2.5",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-split-pane": "^0.1.65",
    "react-syntax-highlighter": "^5.1.3",
    "react-tooltip": "^3.3.0",
    "redux": "^3.5.2",
    "secure-random": "^1.1.1",
    "wif": "^2.0.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "electron": "^1.6.11",
    "electron-builder": "^19.17.0",
    "electron-packager": "^8.6.0",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.1",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.16.1",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.2",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "__tests__/setupTests.js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setupTests.js"
  }
}
