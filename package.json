{
  "name": "Neon",
  "version": "2.23.10",
  "main": "./main.js",
  "description": "Light wallet for NEO blockchain",
  "homepage": "https://github.com/CityOfZion/neon-wallet",
  "author": "Ethan Fast <ejhfast@gmail.com> (https://github.com/Ejhfast)",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env NODE_ENV=production electron .",
    "assets": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod --progress",
    "dev": "cross-env START_HOT=1 NODE_ENV=development node_modules/.bin/webpack-dev-server --progress --config ./config/webpack.config.dev",
    "start-dev": "cross-env NODE_ENV=development electron . --enable-logging --remote-debugging-port=9222 --progress",
    "assets-dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.dev --progress",
    "test": "./node_modules/.bin/jest --coverage --no-watchman",
    "test-ci": "yarn test --maxWorkers=4 --watchAll=false",
    "test:e2e": "ava __e2e__/*.e2e.js --timeout=120s -s",
    "test-watch": "jest --watch",
    "pack": "build --dir",
    "create-release:patch": "node scripts/create-release.js patch",
    "create-release:minor": "node scripts/create-release.js minor",
    "create-release:major": "node scripts/create-release.js major",
    "release": "yarn assets && yarn electron-builder --publish always",
    "release:mac": "yarn assets && yarn electron-builder --publish always --mac --x64 --arm64",
    "build:win": "yarn assets && yarn electron-builder --win",
    "dist": "yarn assets && dotenv yarn electron-builder",
    "lint": "eslint -c .eslintrc ./app --ext .js,.jsx --ignore-pattern dist/",
    "lint-fix": "eslint -c .eslintrc ./app --ext .js,.jsx --fix --ignore-pattern dist/",
    "prepush": "echo 'running flow...' && yarn flow && echo 'running lint...' && yarn lint && echo 'running tests...' && yarn test",
    "electron-rebuild": "electron-rebuild --force --module_dir . -w node-hid",
    "flow": "flow check",
    "format": "prettier --write '**/*.{js,jsx,.scss,.css}'",
    "clean": "rm -r app/dist/*"
  },
  "keywords": [],
  "license": "MIT",
  "build": {
    "appId": "com.electron.neon",
    "files": [
      "./preload.js",
      "./main.js",
      "app/dist/index.html",
      "app/dist/bundle.js",
      "app/dist/style.css"
    ],
    "afterSign": "electron-builder-notarize",
    "publish": [
      {
        "provider": "github",
        "owner": "CityOfZion",
        "repo": "neon-wallet"
      }
    ],
    "dmg": {
      "background": "./app/assets/images/background.tiff",
      "window": {
        "width": 576,
        "height": 404
      },
      "contents": [
        {
          "x": 168,
          "y": 200
        },
        {
          "x": 408,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "hardenedRuntime": true,
      "entitlements": "mac-config/entitlements.mac.plist",
      "entitlementsInherit": "mac-config/entitlements.mac.plist",
      "artifactName": "${productName}.${version}.${arch}.${ext}",
      "type": "distribution",
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      }
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "artifactName": "${productName}.${version}.${ext}"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "artifactName": "${productName}.${version}.${ext}",
      "verifyUpdateCodeSignature": false
    },
    "protocols": {
      "name": "neon-protocol",
      "schemes": [
        "neon"
      ]
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/helper-get-function-arity": "^7.16.7",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@cfaester/enzyme-adapter-react-18": "^0.6.0",
    "@chakra-ui/react": "^2.5.1",
