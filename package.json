{
  "name": "neon",
  "version": "1.0.0",
  "description": "Light wallet for NEO blockchain",
  "homepage": "https://github.com/CityOfZion/neon-wallet",
  "author": "Coz (https://github.com/CityOfZion)",
  "keywords": [],
  "license": "MIT",
  "main": "./out/main/index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cityofzion/blockchain-service": "0.7.1",
    "@cityofzion/bs-asteroid-sdk": "0.7.1",
    "@cityofzion/bs-ethereum": "0.7.1",
    "@cityofzion/bs-neo-legacy": "0.7.1",
    "@cityofzion/bs-neo3": "0.7.1",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@reduxjs/toolkit": "^1.9.5",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "electron-updater": "^6.1.1",
    "i18next": "^23.5.1",
    "react-i18next": "^13.3.0",
    "react-icons": "^4.11.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "redux-persist": "^6.0.0",
    "vite-plugin-node-polyfills": "^0.14.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/eslint-plugin-query": "^4.34.1",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.66.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0"
  }
}
