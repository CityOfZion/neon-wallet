    "@radix-ui/react-tooltip": "^1.0.7",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/electron": "^4.14.0",
    "@sentry/react": "^7.83.0",
    "@tanstack/react-query": "^5.0.5",
    "@tanstack/react-table": "^8.12.0",
